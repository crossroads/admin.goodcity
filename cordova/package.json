{
  "name": "hk.goodcity.adminstaging",
  "version": "0.20.6",
  "displayName": "S. Admin GoodCity",
  "dependencies": {},
  "devDependencies": {
    "@havesource/cordova-plugin-push": "^3.0.0",
    "cordova-android": "^10.1.1",
    "cordova-android-support-gradle-release": "^3.0.1",
    "cordova-ios": "^6.2.0",
    "cordova-plugin-actionsheet": "^2.3.3",
    "cordova-plugin-android-permissions": "^1.1.2",
    "cordova-plugin-androidx-adapter": "^1.1.3",
    "cordova-plugin-camera": "^6.0.0",
    "cordova-plugin-device": "^2.0.3",
    "cordova-plugin-inappbrowser": "^5.0.0",
    "cordova-plugin-network-information": "^3.0.0",
    "cordova-plugin-splashscreen": "^6.0.0",
    "cordova-plugin-statusbar": "^3.0.0",
    "cordova-plugin-twiliovoicesdk": "^6.0.3",
    "es6-promise-plugin": "^4.2.2",
    "phonegap-plugin-barcodescanner": "^8.1.0",
    "phonegap-plugin-media-stream": "git+https://github.com/Kacoze/phonegap-plugin-media-stream.git"
  },
  "cordova": {
    "platforms": [
      "ios",
      "android"
    ],
    "plugins": {
      "cordova-android-support-gradle-release": {
        "ANDROID_SUPPORT_VERSION": "30"
      },
      "cordova-plugin-twiliovoicesdk": {
        "INCOMING_CALL_APP_NAME": "GoodCity.HK",
        "MASK_INCOMING_PHONE_NUMBER": "true",
        "ENABLE_CALL_KIT": "false"
      },
      "cordova-plugin-actionsheet": {},
      "cordova-plugin-android-permissions": {},
      "cordova-plugin-camera": {
        "ANDROIDX_CORE_VERSION": "1.6.+"
      },
      "cordova-plugin-device": {},
      "cordova-plugin-inappbrowser": {},
      "cordova-plugin-network-information": {},
      "cordova-plugin-splashscreen": {},
      "cordova-plugin-statusbar": {},
      "es6-promise-plugin": {},
      "phonegap-plugin-barcodescanner": {
        "ANDROID_SUPPORT_V4_VERSION": "27.+"
      },
      "@havesource/cordova-plugin-push": {
        "ANDROIDX_CORE_VERSION": "1.6.+",
        "FCM_VERSION": "18.+"
      },
      "cordova-plugin-androidx-adapter": {},
      "phonegap-plugin-media-stream": {}
    }
  }
}
